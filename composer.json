{
    "name": "payjp/payjpv2-php",
    "description": "PAY.JP API v2 PHP SDK",
    "config": {
        "platform": {
            "php": "8.1"
        }
    },
    "keywords": [
        "openapitools",
        "openapi-generator",
        "openapi",
        "php",
        "sdk",
        "rest",
        "api"
    ],
    "homepage": "https://github.com/payjp/payjpv2-php",
    "license": "MIT",
    "authors": [
        {
            "name": "PAY.JP",
            "homepage": "https://pay.jp"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^1.7 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^3.5"
    },
    "autoload": {
        "psr-4": { "PAYJPV2\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "PAYJPV2\\Test\\" : "test/" }
    }
}
